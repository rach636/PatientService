{
  "name": "patient-service",
  "version": "1.0.0",
  "description": "Patient management microservice for Hospital Management System",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "hospital",
    "patient",
    "microservice",
    "healthcare"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "pg": "^8.11.3",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": "^18.0.0",
    "npm": "^9.0.0"
  }
}
